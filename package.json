{
  "name": "html2notebook",
  "version": "1.0.0",
  "author": "Mara Nikola Kiefer",
  "license": "MIT",
  "description": "Convert static HTML files to notebooks",
  "main": "lib/index.js",
  "files": [
    "./lib"
  ],
  "types": "lib/types.d.js",
  "jest": {
    "projects": [
      "test/jest.unit.config.js",
      "test/jest.int.config.js"
    ]
  },
  "dependencies": {
    "htmlparser2": "^9.0.0",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "eslint": "^8.43.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29",
    "jsdoc-to-markdown": "^8.0.0",
    "vitepress": "^1.0.0-beta.3"
  },
  "scripts": {
    "test": "jest --projects test/jest.unit.config.js",
    "test:int": "jest --projects test/jest.int.config.js",
    "lint": "eslint",
    "jsdoc": "npx jsdoc2md lib/index.js > docs/api.md",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  }
}
